{
    "title": "YNC的博客文档",
    "author": "YNC",
    "description": "YNC的博客文档",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-search",
        "search-pro",
        "tbfed-pagefooter",
        "livereload",
        "splitter",
        "highlight",
        "github",
        "sharing-plus",
        "-lunr",
        "code",
        "copy-code-button",
        "back-to-top-button",
        "prism",
        "click-reveal"
    ],
    "pluginsConfig": {
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        },
        "tbfed-pagefooter": {
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "github": {
            "url": "https://github.com/yunaichun"
        }
    },
    "links": {
        "sidebar": {
            "Github": "https://github.com/yunaichun"
        },
        "sharing": {
            "google": false,
            "facebook": false,
            "twitter": false,
            "weibo": false,
            "all": false
        }
    }
}